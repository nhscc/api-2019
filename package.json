{
  "name": "elections.api.hscc.bdpa.org",
  "description": "https://elections.api.hscc.bdpa.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhscc/elections.api.hscc.bdpa.org"
  },
  "version": "2.0.0",
  "scripts": {
    "dev": "gulp checkEnv && next -p `npx -q get-clean-port`",
    "build": "gulp checkEnv && next build",
    "start": "gulp checkEnv && next start",
    "test": "jest",
    "install-types": "flow-typed install",
    "debug-remote": "node --inspect-brk='0.0.0.0:7777'",
    "debug-remote-gulp": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/gulp",
    "debug-remote-jest": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/jest --runInBand",
    "test-ib": "jest --runInBand",
    "test-nc": "jest --no-cache",
    "clean-types": "gulp cleanTypes",
    "generate": "BABEL_ENV=generator babel config --out-dir .",
    "regenerate": "gulp regenerate",
    "list-tasks": "npm run && echo 'Compiling gulp task list...' && gulp --tasks",
    "preinstall": "npx npm-force-resolutions"
  },
  "author": "Xunnamius",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "moduleNameMapper": {
      "^universe/(.*)$": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "resolutions": {
    "yargs-parser": "^13.1.2"
  },
  "expectedEnvVariables": [
    "NODE_ENV|BABEL_ENV|APP_ENV"
  ],
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "confusing-browser-globals": "^1.0.9",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.4.0",
    "next-transpile-modules": "^3.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-source-map-support": "^2.1.1",
    "del": "^5.1.0",
    "eslint": "^7.0.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "fancy-log": "^1.3.3",
    "flow-bin": "^0.124.0",
    "flow-typed": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-tap": "^2.0.0",
    "jest": "^26.0.1",
    "parse-gitignore": "^1.0.1",
    "replace-in-file": "^6.0.0",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.19",
    "vercel": "^19.0.1"
  }
}
